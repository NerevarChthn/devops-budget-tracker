{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"Add-Transaction": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/add-transaction",
			"sourceRoot": "projects/add-transaction/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/add-transaction",
						"index": "projects/add-transaction/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/add-transaction/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/add-transaction/public",
								"output": "."
							}
						],
						"styles": [
							"projects/add-transaction/src/styles.css"
						],
						"scripts": [],
						"main": "projects/add-transaction/src/main.ts",
						"extraWebpackConfig": "projects/add-transaction/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/add-transaction/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "Add-Transaction:build:production",
							"extraWebpackConfig": "projects/add-transaction/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "Add-Transaction:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4300,
						"publicHost": "http://localhost:4300",
						"extraWebpackConfig": "projects/add-transaction/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/add-transaction/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/add-transaction/public"
							}
						],
						"styles": [
							"projects/add-transaction/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
		"data-models": {
			"projectType": "library",
			"root": "projects/data-models",
			"sourceRoot": "projects/data-models/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/data-models/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/data-models/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/data-models/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"tsConfig": "projects/data-models/tsconfig.spec.json",
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						]
					}
				}
			}
		},
		"Budget-List": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/budget-list",
			"sourceRoot": "projects/budget-list/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/budget-list",
						"index": "projects/budget-list/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/budget-list/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/budget-list/public",
								"output": "."
							}
						],
						"styles": [
							"projects/budget-list/src/styles.css"
						],
						"scripts": [],
						"main": "projects/budget-list/src/main.ts",
						"extraWebpackConfig": "projects/budget-list/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/budget-list/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "Budget-List:build:production",
							"extraWebpackConfig": "projects/budget-list/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "Budget-List:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/budget-list/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/budget-list/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/budget-list/public"
							}
						],
						"styles": [
							"projects/budget-list/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	}
}